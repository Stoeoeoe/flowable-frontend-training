{
  "name": "frontend",
  "version": "1.0.0",
  "scripts": {
    "build": "flowbuild build -o ../training-handson-work/src/main/resources/static/ext --outputCss ../training-handson-work/src/main/resources/static/ext",
    "watch": "flowbuild watch -o ../training-handson-work/src/main/resources/static/ext --outputCss ../training-handson-work/src/main/resources/static/ext",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@flowable/work-scripts": "3.11.5",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-storysource": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "storybook-addon-mock": "^3.2.0",
    "babel-loader": "^9.1.2",
    "concurrently": "^7.6.0",
    "cpx": "^1.5.0",
    "rollup": "^3.18.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "compilerOptions": {
    "jsx": "react",
    "module": "esNext",
    "moduleResolution": "node",
    "outDir": "./dist",
    "preserveConstEnums": true,
    "strict": true,
    "strictFunctionTypes": false,
    "target": "es5",
    "sourceMap": true,
    "importHelpers": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}
