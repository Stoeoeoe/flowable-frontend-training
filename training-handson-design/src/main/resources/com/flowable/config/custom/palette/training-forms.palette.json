{
  "$schema": "https://developer-docs.flowable.com/schemas/palette.json",
  "Palette-Id": "flowable-frontend-palette",
  "patchPalettes": [
    "flowable-work-form-palette",
    "flowable-work-page-palette"
  ],
  "resourceBundles": [
    "com/flowable/config/custom/palette/training-i18n"
  ],
  "groups": {
    "training": {
      "index": 1
    }
  },
  "presentations": [
    {
      "id": "hello-world.presentation",
      "bigIcon": "component-presentations/palette-icons/hello-world.svg",
      "icon": "component-presentations/palette-icons/hello-world.svg"
    }
  ],
  "stencils": [
    {
      "id": "hello-world",
      "xmlTag": "trainingHelloworld",
      "title": "Hello World",
      "description": "Custom greeting component with optional label",
      "viewPath": "component-presentations/editor-views/htmlComponent.svg",
      "superId": "Component",
      "presentationId": "hello-world.presentation",
      "groups": [
        "training"
      ],
      "propertyPackages": [
        "labelWithDefaultValueAttributes"
      ],
      "properties": [
        {
          "id": "value",
          "visible": false
        },
        {
          "id": "default-value",
          "visible": false
        },
        {
          "id": "locationToGreet",
          "title": "Location to Greet",
          "value": "World",
          "category": "details",
          "type": "SimpleTextExpression",
          "path": "extraSettings.locationToGreet",
          "runtime": true,
          "index": 10
        },
        {
          "id": "greeting",
          "title": "Greeting",
          "value": "Hello",
          "category": "details",
          "type": "SimpleTextExpression",
          "path": "extraSettings.greeting",
          "index": 20,
          "optional": false
        }
      ]
    },
    {
      "id": "map",
      "xmlTag": "trainingFlowableMap",
      "title": "Map",
      "viewPath": "component-presentations/editor-views/button.svg",
      "bigIcon": "../palette-icons?id=component-presentations/palette-icons/map.svg",
      "icon": "../palette-icons?id=component-presentations/palette-icons/map.svg",
      "superId": "base-component",
      "groups": [
        "training"
      ],
      "layout": [
        {
          "type": "layout.form.controls"
        }
      ],
      "properties": [
        {
          "id": "center",
          "title": "Center",
          "value": "{{[47.37, 8.54]}}",
          "category": "details",
          "type": "SimpleTextExpression",
          "path": "extraSettings.center",
          "index": 10
        }
      ]
    }
  ]
}

