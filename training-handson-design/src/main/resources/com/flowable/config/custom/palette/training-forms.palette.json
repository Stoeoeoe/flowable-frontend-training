{
  "$schema": "https://developer-docs.flowable.com/schemas/palette.json",
  "Palette-Id": "flowable-frontend-palette",
  "patchPalettes": [
    "flowable-form-palette",
    "flowable-page-palette"
  ],
  "resourceBundles": [
    "com/flowable/config/custom/palette/training-i18n"
  ],
  "groups": {
    "training": {
      "title": "Training",
      "index": 999
    }
  },
  "stencils": [
    {
      "id": "hello-world",
      "xmlTag": "trainingHelloworld",
      "title": "Hello World",
      "description": "Custom greeting component with optional label",
      "viewPath": "component-presentations/editor-views/htmlComponent.svg",
      "bigIcon": "../palette-icons?id=component-presentations/palette-icons/hello-world.svg",
      "icon": "../palette-icons?id=component-presentations/palette-icons/hello-world.svg",
      "superId": "base-component",
      "groups": [
        "training"
      ],
      "layout": [
        {
          "type": "layout.form.controls"
        }
      ],
      "properties": [
        {
          "id": "value",
          "visible": false
        },
        {
          "id": "default-value",
          "visible": false
        },
        {
          "id": "locationToGreet",
          "title": "Location to Greet",
          "value": "World",
          "category": "details",
          "type": "SimpleTextExpression",
          "path": "extraSettings.locationToGreet",
          "runtime": true,
          "index": 10
        }
      ]
    }
  ]
}

